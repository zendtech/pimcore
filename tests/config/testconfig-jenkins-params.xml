<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <database>
        <adapter>Pdo_Mysql</adapter>
        <params>
            <host>PIMCORE_DATABASE_HOSTNAME</host>
            <username>PIMCORE_DATABASE_USERNAME</username>
            <password>PIMCORE_DATABASE_PASSWORD</password>
            <dbname>PIMCORE_DATABASE_DATABASE</dbname>
            <port></port>
